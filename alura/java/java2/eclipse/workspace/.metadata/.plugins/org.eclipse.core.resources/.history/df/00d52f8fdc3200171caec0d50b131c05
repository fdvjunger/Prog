
public abstract class Materia {
	
	private String nome;
	private int dps;
	private double n1;
	private double n2;
	private int prerequisito;
	private String prereq;
	private int codigo;
	public String getNome() {
		return nome;
	}
	public void setNome(String nome) {
		this.nome = nome;
	}
	
	
	
}
